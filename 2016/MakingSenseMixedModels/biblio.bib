	Mesh = {Chromosome Mapping; Crosses, Genetic; DNA, Plant; Environment; Genetic Linkage; Genetic Markers; Genotype; Phenotype; Quantitative Trait Loci; Zea mays},
	Month = {Apr},
	Number = {6},
	Pages = {1009-23},
	Pmid = {16538513},
	Pst = {ppublish},
	Title = {Mapping QTLs and QTL x environment interaction for CIMMYT maize drought stress program using factorial regression and partial least squares methods},

	Journal = {Theor Appl Genet},
	Journal-Full = {TAG. Theoretical and applied genetics. Theoretische und angewandte Genetik},
	Mesh = {Agriculture; Australia; Breeding; Crops, Agricultural; Environment; Factor Analysis, Statistical; Models, Statistical; Triticum},
	Month = {Jan},
	Number = {1},
	Pages = {55-72},
	Pmc = {PMC4282718},

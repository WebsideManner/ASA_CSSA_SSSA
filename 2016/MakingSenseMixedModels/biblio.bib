	Url = {http://www.ms-se.com/pt/re/msse/abstract.00005768-200305000-00018.htm;jsessionid=G5rK9QJ8myXX1nLzfqWzrvFgFlP3LJQF141nwQs3QqKZJvyJ4zT0!1330140564!181195629!8091!-1},
